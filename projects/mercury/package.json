{
  "name": "mercury",
  "version": "0.0.0-RELEASEVERSION",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn server:mock\" \"yarn start\" \"yarn test\" \"yarn server\" \"yarn elasticsearch\"",
    "server:mock": "node mock-server/server.js",
    "server": "wait-on http://localhost:9200/ && cd ../saturn/ && ./gradlew run",
    "elasticsearch": "docker run -p 9200:9200 -p 9300:9300 -e \"discovery.type=single-node\" docker.elastic.co/elasticsearch/elasticsearch:6.6.2",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint ./src"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.5",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "latest",
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "date-fns": "next",
    "deepmerge": "^3.2.0",
    "elasticsearch": "^16.0.0",
    "express": "^4.16.4",
    "filesize": "^4.1.2",
    "http-proxy-middleware": "^0.19.1",
    "jsonld": "^1.6.0",
    "lodash": "^4.17.11",
    "material-ui-pickers": "^2.2.4",
    "mdi-material-ui": "latest",
    "prop-types": "^15.7.2",
    "query-string": "^6.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.4",
    "react-redux": "^5.1.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "react-select": "^2.4.3",
    "reduce-reducers": "^0.4.3",
    "redux": "^4.0.1",
    "redux-promise-middleware": "^6.1.0",
    "redux-thunk": "^2.3.0",
    "wait-on": "^3.2.0",
    "webdav": "^2.6.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "redux-mock-store": "^1.5.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
