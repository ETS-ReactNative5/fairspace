{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "rules": {
    "linebreak-style": 0,
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "quote-props": [
      2,
      "consistent-as-needed"
    ],
    "quotes": 0,
    "class-methods-use-this": 0,
    "import/no-named-as-default": 0,
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "object-curly-spacing": [
      "error",
      "never"
    ],
    "comma-dangle": 0,
    "no-console": [
      "error",
      {
        "allow": [
          "warn",
          "error"
        ]
      }
    ],
    "max-len": 0,
    "react/jsx-indent": 0,
    "react/jsx-indent-props": 0,
    "react/sort-comp": 0,
    "react/prop-types": 0,
    "react/destructuring-assignment": 0
  }
}
