# Updates file content
#
# param0 - path
# param1 - size
# param2 - blob id
# param3 - modified by
PREFIX fs: <http://fairspace.io/ontology#>
DELETE { ?s fs:fileSize ?x1 ; fs:blobId ?x2; fs:dateCreated ?x3; fs:dateModified ?x4; fs:modifiedBy ?x5 .}
INSERT { ?s fs:fileSize ?param1; fs:blobId ?param2; fs:dateCreated ?date; fs:modifiedBy ?param3; fs:dateModified ?date }
WHERE { ?s fs:filePath ?param0 ; fs:fileSize ?x1 ; fs:blobId ?x2; fs:dateCreated ?x3; fs:dateModified ?x4; fs:modifiedBy ?x5 . BIND(NOW() as ?date) }