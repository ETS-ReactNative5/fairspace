
# 0 - graph
# 1 - type (optional)

PREFIX fs: <http://fairspace.io/ontology#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

CONSTRUCT {?s a %2$s; rdfs:label ?l}
WHERE {
    GRAPH %1$s { ?s a %2$s }
    GRAPH ws:vocabulary {%2$s fs:fairspaceEntity true .}
    OPTIONAL { GRAPH %1$s {?s rdfs:label ?l} }
}