# Creates a file
#
# 0 - base IRI (string)
# 1 - path
# 2 - size
# 3 - blobId
# 4 - creator

PREFIX fs: <http://fairspace.io/ontology#>

INSERT {
?iri a fs:File;
fs:filePath ?1;
fs:fileSize ?2;
fs:blobId ?3;
fs:createdBy ?4;
fs:dateCreated ?date;
fs:modifiedBy ?4;
fs:dateModified ?date . }
WHERE { BIND(IRI(CONCAT(?0, STRUUID())) as ?iri) BIND(NOW() as ?date) }