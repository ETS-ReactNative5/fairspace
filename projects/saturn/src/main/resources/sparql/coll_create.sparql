# Creates a collection
#
# 0 - IRI
# 1 - pretty name
# 2 - directory name
# 3 - description
# 4 - type
# 5 - creator

PREFIX fs: <http://fairspace.io/ontology#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

INSERT {
  ?0 a fs:Collection;
  rdfs:label ?1;
  fs:filePath ?2;
  rdfs:comment ?3;
  fs:collectionType ?4;
  fs:fileSize 0;
  fs:createdBy ?5;
  fs:dateCreated ?date;
  fs:modifiedBy ?5;
  fs:dateModified ?date . }
WHERE { BIND(NOW() as ?date) };


