@prefix fs: <http://fairspace.io/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://www.schema.org/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix osiris: <http://osiris.fairspace.io/ontology#> .
@prefix icdo3: <http://icdo3.fairspace.io/ontology#> .
@prefix fhir: <http://hl7.org/fhir/STU3/> .

########################
###   User Shapes    ###
########################

## Augmented system class shapes
fs:FileShape sh:property
    fs:associatedAnalysisShape .

fs:CollectionShape sh:property
    fs:aboutPersonShape,
    fs:aboutMaterialShape,
    fs:partOfProjectShape .


## User class Shapes

foaf:PersonShape a fs:ClassShape ;
    sh:closed false ;
    sh:description "A person that is subject of research." ;
    sh:name "Person" ;
    sh:targetClass foaf:Person ; 
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    fs:showInCatalog true ;
    sh:property
        fhir:AdministrativeGenderShape,
        fs:ageOfDeathShape,
        fs:patientIdentifierShape,
        fs:hasEventShape,
        rdfs:labelShape,
        rdfs:commentShape .

fs:PersonConsentShape a fs:ClassShape ;
    sh:closed false ; 
    sh:description "A research consent given by person." ;
    sh:name "Person Consent" ;
    sh:targetClass fs:PersonConsent ; 
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    fs:showInCatalog true ;
    sh:property
        fs:isProjectConsentShape,
        fs:authorizesShape,
        rdfs:labelShape,
        rdfs:commentShape .

fs:ResearchProjectShape a fs:ClassShape ;
    sh:closed false ; 
    sh:description "A research project." ;
    sh:name "Research Project" ;
    sh:targetClass fs:ResearchProject ; 
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    fs:showInCatalog true ;
    sh:property
        fs:projectCodeShape,
        fs:investigationNatureShape, 
        fs:projectManagerShape,
        fs:hasCollectionShape, 
        fs:hasPersonConsentShape,
        rdfs:labelShape,
        rdfs:commentShape .

osiris:TumorPathologyEventShape a fs:ClassShape ;
    sh:closed false ; 
    sh:description "Description for osiris:TumorPathologyEvent" ;
    sh:name "Tumor Pathology Event" ;
    sh:targetClass osiris:TumorPathologyEvent ; 
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    fs:showInCatalog true ;
    sh:property
        osiris:EventTypeShape,
        osiris:EventStartDateShape,
        osiris:EventEndDateShape,
        osiris:HistologicalGradeTypeShape,
        osiris:HistologicalGradeValueShape,
        fs:diagnosisDateShape,
        icdo3:TopographyShape,
        icdo3:MorphologyShape,
        osiris:LateralityShape,
        fs:isAnalysedByShape,
        fs:eventSubjectShape,
        fs:isLinkedToShape,
        rdfs:labelShape,
        rdfs:commentShape .

osiris:BiologicalSampleShape a fs:ClassShape ;
    sh:closed false ; 
    sh:description "Description for osiris:BiologicalSample" ;
    sh:name "Biological Sample" ;
    sh:targetClass osiris:BiologicalSample ; 
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    fs:showInCatalog true ;
    sh:property
        fs:SampleExternalAccessionShape,
        fs:collectDateShape,
        osiris:SampleNatureShape,
        osiris:SampleOriginShape,
        osiris:StorageTemperatureShape,
        fs:tumorCellularityShape,
        fs:isAuthorizedByShape,
        fs:isParentOfShape,
        fs:isChildOfShape,
        rdfs:labelShape,
        rdfs:commentShape .

fs:AnalysisShape a fs:ClassShape ;
    sh:closed false ; 
    sh:description "Description for fs:Analysis" ;
    sh:name "Analysis" ;
    sh:targetClass fs:Analysis ; 
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    fs:showInCatalog true ;
    sh:property
        fs:analysisCodeShape,
        osiris:AnalysisTypeShape,
        osiris:AnalysisDateShape,
        osiris:TechnicalProtocolShape,
        osiris:PlatformNameShape,
        fs:analyticPipelineCodeShape,
        fs:AlgorithmicCellularityShape,
        osiris:AlgorithmicPloidyShape,
        fs:NumberOfBreakPointsShape,
        fs:associatedFilesShape,
        rdfs:labelShape,
        rdfs:commentShape .

fs:WidgetMaster a fs:Class .
fs:WidgetMasterShape a fs:ClassShape ;
    sh:closed false ; 
    sh:description "A widget is an element of a graphical user interface (GUI) that displays information or provides a specific way for a user to interact with the operating system or an application." ;
    sh:name "Master of widgets" ;
    sh:targetClass fs:WidgetMaster ; 
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    fs:showInCatalog true ;
    sh:property
        fs:relatesToWidgetShape,
        fs:dateWidgetShape,
        fs:dateTimeWidgetShape,
        fs:multiLineStringWidgetShape,
        fs:integerLongWidgetShape,
        fs:integerWidgetShape,
        fs:booleanWidgetShape,
        fs:booleanX4WidgetShape,
        fs:fixedDropdownWidgetShape,
        rdfs:labelShape,
        rdfs:commentShape .

## User property shapes

fs:dateWidget a rdf:Property .
fs:dateWidgetShape a fs:PropertyShape ; 
    sh:name "A single date" ;
    sh:description "A single date." ;
    sh:datatype xsd:date ;
    sh:maxCount 1 ;
    sh:path fs:dateWidget .

fs:dateTimeWidget a rdf:Property .
fs:dateTimeWidgetShape a fs:PropertyShape ; 
    sh:name "Date and times" ;
    sh:description "A bunch of date and times." ;
    sh:datatype xsd:dateTime ;
    sh:path fs:dateTimeWidget .

fs:multiLineStringWidget a rdf:Property .
fs:multiLineStringWidgetShape a fs:PropertyShape ; 
    sh:name "Multi line string" ;
    sh:description "Can do newlines, but max length of 500." ;
    sh:datatype xsd:string ;
    sh:maxLength 500 ;
    sh:path fs:multiLineStringWidget .

fs:integerLongWidget a rdf:Property .
fs:integerLongWidgetShape a fs:PropertyShape ; 
    sh:name "Integer" ;
    sh:description "A single integer (long)." ;
    sh:datatype xsd:long ;
    sh:maxCount 1 ;
    sh:path fs:integerLongWidget .

fs:integerWidget a rdf:Property .
fs:integerWidgetShape a fs:PropertyShape ; 
    sh:name "Integer" ;
    sh:description "A single integer." ;
    sh:datatype xsd:integer ;
    sh:maxCount 1 ;
    sh:path fs:integerWidget .

fs:booleanWidget a rdf:Property .
fs:booleanWidgetShape a fs:PropertyShape ; 
    sh:name "Boolean" ;
    sh:description "On or off single." ;
    sh:datatype xsd:boolean ;
    sh:maxCount 1 ;
    sh:path fs:booleanWidget .

fs:booleanX4Widget a rdf:Property .
fs:booleanX4WidgetShape a fs:PropertyShape ; 
    sh:name "Boolean X4" ;
    sh:description "On or off 4 times." ;
    sh:datatype xsd:boolean ;
    sh:maxCount 4 ;
    sh:path fs:booleanX4Widget .

fs:fixedDropdownWidget a rdf:Property .
fs:fixedDropdownWidgetShape a fs:PropertyShape ; 
    sh:name "Fixed set of values (pick 2)" ;
    sh:description "A set of 2 values from a fixed list." ;
    sh:in ( "E" "A" "D" "G" "B" "E" ) ; 
    sh:maxCount 2 ;
    sh:path fs:fixedDropdownWidget .

fs:projectCodeShape a fs:PropertyShape ;
    sh:name "Project Code" ;
    sh:description "Specific project code." ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path fs:projectCode . 
     
fs:investigationNatureShape a fs:PropertyShape ;
    sh:name "Investigation Nature" ;
    sh:description "choose between: basic research, biomedical research, clinical trial, medical diagnosis" ;
    sh:datatype xsd:string ;
    sh:maxLength 128 ;
    sh:path fs:investigationNature . 
     
fs:projectManagerShape a fs:PropertyShape ;
    sh:name "Project Manager" ;
    sh:description "Project manager" ;
    sh:datatype xsd:string ;
    sh:maxLength 128 ;
    sh:path fs:projectManager . 
     
fhir:AdministrativeGenderShape a fs:PropertyShape ;
    sh:name "Gender" ;
    sh:description "The gender of the patient" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path fhir:AdministrativeGender . 
     
fs:ageOfDeathShape a fs:PropertyShape ;
    sh:name "Age of death" ;
    sh:description "The age at which this person has died" ;
    sh:datatype xsd:date ;
    sh:maxCount 1 ;
    sh:path fs:ageOfDeath . 
     
fs:patientIdentifierShape a fs:PropertyShape ;
    sh:name "Patient Identifier" ;
    sh:description "The identifier of the patient" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path fs:patientIdentifier . 
     
osiris:EventTypeShape a fs:PropertyShape ;
    sh:name "Neoplasm Event" ;
    sh:description "The type of tumor event (primary tumor, local relapse, metastatic relapse)" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path osiris:EventType . 
     
osiris:EventStartDateShape a fs:PropertyShape ;
    sh:name "Neoplasm Event Start Date" ;
    sh:description "The start date of the tumor event" ;
    sh:datatype xsd:date ;
    sh:maxCount 1 ;
    sh:path osiris:EventStartDate . 
     
osiris:EventEndDateShape a fs:PropertyShape ;
    sh:name "Neoplasm Event End Date" ;
    sh:description "The end date of the tumor event" ;
    sh:datatype xsd:date ;
    sh:maxCount 1 ;
    sh:path osiris:EventEndDate . 
     
osiris:HistologicalGradeTypeShape a fs:PropertyShape ;
    sh:name "Neoplasm Grade Type" ;
    sh:description "The grade type of the tumor" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path osiris:HistologicalGradeType . 
     
osiris:HistologicalGradeValueShape a fs:PropertyShape ;
    sh:name "Neoplasm Grade Value" ;
    sh:description "The grade value of the tumor" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path osiris:HistologicalGradeValue . 
     
fs:diagnosisDateShape a fs:PropertyShape ;
    sh:name "Neoplasm diagnosis date" ;
    sh:description "The date of tumor diagnosis" ;
    sh:datatype xsd:date ;
    sh:maxCount 1 ;
    sh:path fs:diagnosisDate . 
     
icdo3:TopographyShape a fs:PropertyShape ;
    sh:name "Neoplasm Topography" ;
    sh:description "The topography of the tumor (ICD-O-3)" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path icdo3:Topography . 
     
icdo3:MorphologyShape a fs:PropertyShape ;
    sh:name "Neoplasm Morphology" ;
    sh:description "The morphology of the tumor (ICD-O-3)" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path icdo3:Morphology . 
     
osiris:LateralityShape a fs:PropertyShape ;
    sh:name "Neoplasm Laterality" ;
    sh:description "The laterality of the tumor (right, left, bilateral, not applicable)" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path osiris:Laterality . 
     
fs:SampleExternalAccessionShape a fs:PropertyShape ;
    sh:name "Sample External Accession" ;
    sh:description "The biological sample identifier" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path fs:SampleExternalAccession . 
     
fs:collectDateShape a fs:PropertyShape ;
    sh:name "Collect Date" ;
    sh:description "The collect date of the biological sample" ;
    sh:datatype xsd:date ;
    sh:maxCount 1 ;
    sh:path fs:collectDate . 
     
osiris:SampleNatureShape a fs:PropertyShape ;
    sh:name "Sample Nature" ;
    sh:description "The biological sample nature (frozen tissue, FFPE, blood, etc…)" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path osiris:SampleNature . 
     
osiris:SampleOriginShape a fs:PropertyShape ;
    sh:name "Sample Origin" ;
    sh:description "The biological sample origin (tumor or healthy)" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path osiris:SampleOrigin . 
     
osiris:StorageTemperatureShape a fs:PropertyShape ;
    sh:name "Storage Temperature" ;
    sh:description "The storage temperature" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path osiris:StorageTemperature . 
     
fs:tumorCellularityShape a fs:PropertyShape ;
    sh:name "Tumor Cellularity" ;
    sh:description "The percentage of tumor cells in the biological sample (pathological measure)" ;
    sh:datatype xsd:integer ;
    sh:maxCount 1 ;
    sh:path fs:tumorCellularity . 
     
fs:analysisCodeShape a fs:PropertyShape ;
    sh:name "Analysis code" ;
    sh:description "The analysis run identifier" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path fs:analysisCode . 
     
osiris:AnalysisTypeShape a fs:PropertyShape ;
    sh:name "Analysis type" ;
    sh:description "The type of analysis" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path osiris:AnalysisType . 
     
osiris:AnalysisDateShape a fs:PropertyShape ;
    sh:name "Analysis date" ;
    sh:description "The analysis date" ;
    sh:datatype xsd:date ;
    sh:maxCount 1 ;
    sh:path osiris:AnalysisDate . 
     
osiris:TechnicalProtocolShape a fs:PropertyShape ;
    sh:name "Technical protocol" ;
    sh:description "Protocol used for the experiment" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path osiris:TechnicalProtocol . 
     
osiris:PlatformNameShape a fs:PropertyShape ;
    sh:name "Platform name" ;
    sh:description "Technological platform name (provider followed by the platform name)" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path osiris:PlatformName . 
     
fs:analyticPipelineCodeShape a fs:PropertyShape ;
    sh:name "Analytic pipeline code" ;
    sh:description "Identifier of the pipeline used into the analysis (unique in the center)" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path fs:analyticPipelineCode . 
     
fs:AlgorithmicCellularityShape a fs:PropertyShape ;
    sh:name "Algorithmic cellularity" ;
    sh:description "Calculated percentage of tumor cells in sample" ;
    sh:datatype xsd:integer ;
    sh:maxCount 1 ;
    sh:path fs:AlgorithmicCellularity . 
     
osiris:AlgorithmicPloidyShape a fs:PropertyShape ;
    sh:name "Algorithmic ploidy" ;
    sh:description "Calculated number of sets of chromosomes in a cell of the sample" ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 128 ;
    sh:path osiris:AlgorithmicPloidy . 
     
fs:NumberOfBreakPointsShape a fs:PropertyShape ;
    sh:name "Number of break points" ;
    sh:description "Calculated number of break points in genomic profile" ;
    sh:datatype xsd:integer ;
    sh:maxCount 1 ;
    sh:path fs:NumberOfBreakPoints . 

## User relation shapes

fs:relatesToWidget a rdf:Property .
fs:relatesToWidgetShape a fs:RelationShape ; 
    sh:name "Relates to widget master" ;
    sh:description "Relates to other widget masters" ;
    sh:class fs:WidgetMaster ; 
    fs:inverseRelation fs:relatesToWidgetShape ;
    sh:path fs:relatesToWidget .

fs:derivesFromShape a fs:RelationShape ; 
    sh:name "Derives from" ;
    sh:description "Person that provided this sample." ;
    sh:class foaf:Person ; 
    sh:maxCount 1 ;
    fs:inverseRelation fs:providesMaterialShape ;
    sh:path fs:derivesFrom .

fs:providesMaterialShape a fs:RelationShape ; 
    sh:name "Provides material" ;
    sh:description "Samples provided by this person" ;
    sh:class osiris:BiologicalSample ; 
    fs:inverseRelation fs:derivesFromShape ;
    sh:path fs:providesMaterial .

fs:isConsentOfShape a fs:RelationShape ; 
    sh:name "Is consent of" ;
    sh:description "" ;
    sh:class foaf:Person ; 
    sh:maxCount 1 ;
    fs:inverseRelation fs:gaveConsentShape ;
    sh:path fs:isConsentOf .

fs:gaveConsentShape a fs:RelationShape ; 
    sh:name "Gave consent" ;
    sh:description "" ;
    sh:class fs:PersonConsent ; 
    fs:inverseRelation fs:isConsentOfShape ; 
    sh:path fs:gaveConsent .

fs:hasCollectionShape a fs:RelationShape ; 
    sh:name "Has associated collections" ; 
    sh:description "Collections that are related to this project." ;
    sh:class fs:Collection ; 
    fs:inverseRelation fs:partOfProjectShape ; 
    sh:path fs:hasCollection .

fs:partOfProjectShape a fs:RelationShape ; 
    sh:name "Part of project" ;
    sh:description "" ;
    sh:class fs:ResearchProject ; 
    sh:maxCount 1 ;
    fs:inverseRelation fs:hasCollectionShape ; 
    sh:path fs:partOfProject .

fs:aboutPersonShape a fs:RelationShape ; 
    sh:name "Is about person" ;
    sh:description "Persons that are featured in this collection." ;
    sh:class foaf:Person ;
    fs:inverseRelation fs:hasDataShape ; 
    sh:path fs:aboutPerson .

fs:hasDataShape a fs:RelationShape ; 
    sh:name "Has data in collection" ;
    sh:description "Collections that contain data for this person." ;
    sh:class fs:Collection ;
    fs:inverseRelation fs:aboutPersonShape ;
    sh:path fs:hasData .

fs:aboutMaterialShape a fs:RelationShape ; 
    sh:name "Is about biological sample." ;
    sh:description "Materials that are featured in this collection." ;
    sh:class osiris:BiologicalSample ; 
    sh:path fs:aboutMaterial .

fs:hasDataSShape a fs:RelationShape ; 
    sh:name "Has data in collection" ;
    sh:description "Collections that contain data for this sample." ;
    sh:class fs:Collection ;
    fs:inverseRelation fs:aboutMaterialShape ;
    sh:path fs:hasDataS .

fs:isProjectConsentShape a fs:RelationShape ; 
    sh:name "Consent for specific project" ;
    sh:description "" ;
    sh:class fs:ResearchProject ; 
    fs:inverseRelation fs:hasPersonConsentShape ; 
    sh:path fs:isProjectConsent .

fs:hasPersonConsentShape a fs:RelationShape ; 
    sh:name "Person consent" ;
    sh:description "" ;
    sh:class fs:PersonConsent ; 
    fs:inverseRelation fs:isProjectConsentShape ; 
    sh:path fs:hasPersonConsent .

fs:isAnalysedByShape a fs:RelationShape ; 
    sh:name "Event is analysed by" ;
    sh:description "Analyses associated with this tumor pathology event" ;
    sh:class fs:Analysis ; 
    fs:inverseRelation fs:analysisOfEventShape ;
    sh:path fs:isAnalysedBy .

fs:analysisOfEventShape a fs:RelationShape ; 
    sh:name "Analysis of tumor event" ;
    sh:description "Event associated with this analysis" ;
    sh:class osiris:TumorPathologyEvent ; 
    sh:maxCount 1 ;
    fs:inverseRelation fs:isAnalysedByShape ;
    sh:path fs:analysisOfEvent .

fs:authorizesShape a fs:RelationShape ; 
    sh:name "Consent authorizes" ;
    sh:description "The samples that is authorized with this consent" ;
    sh:class osiris:BiologicalSample ; 
    fs:inverseRelation fs:isAuthorizedByShape ;
    sh:path fs:authorizes .

fs:isAuthorizedByShape a fs:RelationShape ; 
    sh:name "Sample is authorized by" ;
    sh:description "The relevent consent" ;
    sh:class fs:PersonConsent ; 
    sh:maxCount 1 ;
    fs:inverseRelation fs:authorizesShape ;
    sh:path fs:isAuthorizedBy .

fs:hasEventShape a fs:RelationShape ; 
    sh:name "Event" ;
    sh:description "Tumor pathology events for this patient" ;
    sh:class osiris:TumorPathologyEvent ; 
    fs:inverseRelation fs:eventSubjectShape ;
    sh:path fs:hasEvent .

fs:eventSubjectShape a fs:RelationShape ; 
    sh:name "Event subject" ;
    sh:description "The patient associated with this event" ;
    sh:class foaf:Person ; 
    sh:maxCount 1 ;
    fs:inverseRelation fs:hasEventShape ;
    sh:path fs:eventSubject .

fs:associatedAnalysisShape a fs:RelationShape ; 
    sh:name "Analysed by" ;
    sh:description "The files associated to the analysis" ;
    sh:class fs:Analysis ; 
    sh:maxCount 1 ;
    fs:inverseRelation fs:associatedFilesShape ;
    sh:path fs:associatedAnalysis .

fs:associatedFilesShape a fs:RelationShape ; 
    sh:name "Analysis files" ;
    sh:description "The analysis linked to the files" ;
    sh:class fs:File ; 
    fs:inverseRelation fs:associatedAnalysisShape ;
    sh:path fs:associatedFiles .

fs:isParentOfShape a fs:RelationShape ; 
    sh:name "Sample has children" ;
    sh:description "The biological sample has child samples extracted (ex : tissue DNA extraction)" ;
    sh:class osiris:BiologicalSample ; 
    fs:inverseRelation fs:isChildOfShape ; 
    sh:path fs:isParentOf .

fs:isChildOfShape a fs:RelationShape ; 
    sh:name "Sample is child sample of" ;
    sh:description "The biological sample has been extracted from this parent sample" ;
    sh:class osiris:BiologicalSample ; 
    fs:inverseRelation fs:isParentOfShape ;
    sh:path fs:isChildOf .

fs:isLinkedToShape a fs:RelationShape ; 
    sh:name "Event parentage" ;
    sh:description "The tumor events could be linked to others as a chain. For example, a primary tumor event could be linked to a local relapse and then a metastatic relapse." ;
    sh:class osiris:TumorPathologyEvent ; 
    fs:inverseRelation fs:isLinkedToShape ;
    sh:path fs:isLinkedTo .


