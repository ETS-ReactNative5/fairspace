## User relations

fs:hasData a rdf:Property .
fs:aboutPerson a rdf:Property .
fs:aboutMaterial a rdf:Property .

fs:derivesFrom a rdf:Property ;
    owl:inverseOf fs:providesMaterial .

fs:providesMaterial a rdf:Property ;
    owl:inverseOf fs:derivesFrom .

fs:isConsentOf a rdf:Property ;
    owl:inverseOf fs:gaveConsent .

fs:gaveConsent a rdf:Property ;
    owl:inverseOf fs:isConsentOf .

fs:hasCollection a rdf:Property ;
    owl:inverseOf fs:partOfProject .

fs:partOfProject a rdf:Property ;
    owl:inverseOf fs:hasCollection .

fs:isProjectConsent a rdf:Property ;
    owl:inverseOf fs:hasPersonConsent .

fs:hasPersonConsent a rdf:Property ;
    owl:inverseOf fs:isProjectConsent .

fs:isAnalysedBy a rdf:Property ;
    owl:inverseOf fs:analysisOfEvent .

fs:analysisOfEvent a rdf:Property ;
    owl:inverseOf fs:isAnalysedBy .

fs:authorizes a rdf:Property ;
    owl:inverseOf fs:isAuthorizedBy .

fs:isAuthorizedBy a rdf:Property ;
    owl:inverseOf fs:authorizes .

fs:hasEvent a rdf:Property ;
    owl:inverseOf fs:eventSubject .

fs:eventSubject a rdf:Property ;
    owl:inverseOf fs:hasEvent .

fs:associatedAnalysis a rdf:Property ;
    owl:inverseOf fs:associatedFiles .

fs:associatedFiles a rdf:Property ;
    owl:inverseOf fs:associatedAnalysis .

fs:isParentOf a rdf:Property ;
    owl:inverseOf fs:isChildOf .

fs:isChildOf a rdf:Property ;
    owl:inverseOf fs:isParentOf .

