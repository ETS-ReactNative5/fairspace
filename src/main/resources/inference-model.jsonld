{
    "@context": {
        "owl": "http://www.w3.org/2002/07/owl#",
        "fairspace": "http://fairspace.io/ontology#",
        "schema": "http://schema.org/"
    },
    "@graph": [
        {
            "@id": "fairspace:derivesFrom",
            "owl:inverseOf": {
                "@id": "fairspace:providesMaterial"
            }
        },
        {
            "@id": "fairspace:isConsentOf",
            "owl:inverseOf": {
                "@id": "fairspace:gaveConsent"
            }
        },
        {
            "@id": "fairspace:isProjectConsent",
            "owl:inverseOf": {
                "@id": "fairspace:hasPersonConsent"
            }
        },
        {
            "@id": "fairspace:hasCollection",
            "owl:inverseOf": {
                "@id": "fairspace:hasPersonConsent"
            }
        },
        {
            "@id": "fairspace:partOf",
            "owl:inverseOf": {
                "@id": "fairspace:hasPersonConsent"
            }
        }
    ]
}
