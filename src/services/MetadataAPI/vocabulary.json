{
  "@context": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "xsd": "http://www.w3.org/TR/xmlschema11-2/#",
    "dc": "http://purl.org/dc/elements/1.1/",
    "schema": "http://schema.org/",
    "fairspace": "http://fairspace.io/ontology#"
  },
  "@graph": [
    {
      "@id": "fairspace:Collection",
      "@type": "rdf:Class",
      "rdfs:label": "Collection"
    },
    {
      "@id": "schema:Dataset",
      "@type": "rdf:Class",
      "rdfs:label": "Dataset"
    },
    {
      "@id": "schema:Person",
      "@type": "rdf:Class",
      "rdfs:label": "Person",
      "fairspace:fairspaceEntity": true
    },
    {
      "@id": "fairspace:User",
      "@type": "rdf:Class",
      "rdfs:label": "User"
    },
    {
      "@id": "schema:Material",
      "@type": "rdf:Class",
      "rdfs:label": "Material",
      "fairspace:fairspaceEntity": true
    },
    {
      "@id": "fairspace:PersonConsent",
      "@type": "rdf:Class",
      "rdfs:label": "Person Consent",
      "fairspace:fairspaceEntity": true
    },
    {
      "@id": "fairspace:ResearchProject",
      "@type": "rdf:Class",
      "rdfs:label": "Research Project",
      "fairspace:fairspaceEntity": true
    },

    {
      "@id": "fairspace:File",
      "@type": "rdf:Class",
      "rdfs:label": "File"
    },
    {
      "@id": "@type",
      "@type": "rdf:Property",
      "rdfs:label": "Type",
      "rdfs:domain": [
        {"@id": "schema:Person"},
        {"@id": "schema:Material"},
        {"@id": "fairspace:PersonConsent"},
        {"@id": "fairspace:ResearchProject"},
        {"@id": "fairspace:File"},
        {"@id": "fairspace:Collection"}
      ],
      "rdfs:range": [{"@id": "rdf:Class"}],
      "fairspace:allowMultiple": true
    },
    {
      "@id": "fairspace:name",
      "@type": "rdf:Property",
      "rdfs:label": "Name",
      "rdfs:domain": [
        {"@id": "fairspace:Collection"},
        {"@id": "schema:Person"}
      ],
      "rdfs:range": [{"@id": "xsd:string"}],
      "fairspace:allowMultiple": false,
      "fairspace:multiLine": false
    },
    {
      "@id": "fairspace:description",
      "@type": "rdf:Property",
      "rdfs:label": "Description",
      "rdfs:domain": [{"@id": "fairspace:Collection"}],
      "rdfs:range": [{"@id": "xsd:string"}],
      "fairspace:allowMultiple": false,
      "fairspace:multiLine": true
    },
    {
      "@id": "schema:creator",
      "@type": "rdf:Property",
      "rdfs:label": "Creator",
      "rdfs:domain": [{"@id": "fairspace:Collection"}],
      "rdfs:range": [{"@id": "fairspace:User"}]
    },
    {
      "@id": "schema:dateCreated",
      "@type": "rdf:Property",
      "rdfs:label": "Date created",
      "rdfs:domain": [{"@id": "fairspace:Collection"}],
      "rdfs:range": [{"@id": "xsd:dateTime"}]
    },
    {
      "@id": "fairspace:derivesFrom",
      "@type": "rdf:Property",
      "rdfs:label": "Derives from",
      "rdfs:domain": [{"@id": "schema:Material"}],
      "rdfs:range": [{"@id": "schema:Person"}],
      "fairspace:allowMultiple": false
    },
    {
      "@id": "fairspace:providesMaterial",
      "@type": "rdf:Property",
      "rdfs:label": "Provides material",
      "rdfs:domain": [{"@id": "schema:Person"}],
      "rdfs:range": [{"@id": "schema:Material"}],
      "fairspace:allowMultiple": true
    },
    {
      "@id": "fairspace:isConsentOf",
      "@type": "rdf:Property",
      "rdfs:label": "Is consent of",
      "rdfs:domain": [{"@id": "fairspace:PersonConsent"}],
      "rdfs:range": [{"@id": "schema:Person"}],
      "fairspace:allowMultiple": false
    },
    {
      "@id": "fairspace:gaveConsent",
      "@type": "rdf:Property",
      "rdfs:label": "Gave consent",
      "rdfs:domain": [{"@id": "schema:Person"}],
      "rdfs:range": [{"@id":  "fairspace:PersonConsent"}],
      "fairspace:allowMultiple": true
    },
    {
      "@id": "fairspace:hasData",
      "@type": "rdf:Property",
      "rdfs:label": "Has data",
      "rdfs:domain": [
        {"@id": "schema:Person"}, 
        {"@id": "schema:Material"}
      ],
      "rdfs:range": [
        {"@id": "fairspace:Collection"},
        {"@id": "fairspace:File"}
      ],
      "fairspace:allowMultiple": true
    },
    {
      "@id": "fairspace:aboutPerson",
      "@type": "rdf:Property",
      "rdfs:label": "Is about person",
      "rdfs:domain": [
        {"@id": "fairspace:Collection"},
        {"@id": "fairspace:File"}
      ],
      "rdfs:range": [{"@id": "schema:Person"}],
      "fairspace:allowMultiple": true
    },
    {
      "@id": "fairspace:aboutMaterial",
      "@type": "rdf:Property",
      "rdfs:label": "Is about material",
      "rdfs:domain": [
        {"@id": "fairspace:Collection"},
        {"@id": "fairspace:File"}
      ],
      "rdfs:range": [{"@id": "schema:Material"}],
      "fairspace:allowMultiple": true
    },
    {
      "@id": "fairspace:hasPart",
      "@type": "rdf:Property",
      "rdfs:label": "Contains file",
      "rdfs:domain": [{"@id": "fairspace:Collection"}],
      "rdfs:range": [{"@id": "fairspace:File"}],
      "fairspace:allowMultiple": true
    },
    {
      "@id": "fairspace:partOf",
      "@type": "rdf:Property",
      "rdfs:label": "Part of collection",
      "rdfs:domain": [{"@id": "fairspace:File"}],
      "rdfs:range": [{"@id": "fairspace:Collection"}],
      "fairspace:allowMultiple": false
    },
    {
      "@id": "fairspace:hasCollection",
      "@type": "rdf:Property",
      "rdfs:label": "Associated collections",
      "rdfs:domain": [{"@id": "fairspace:ResearchProject"}],
      "rdfs:range": [{"@id": "fairspace:Collection"}],
      "fairspace:allowMultiple": true
    },
    {
      "@id": "fairspace:partOfProject",
      "@type": "rdf:Property",
      "rdfs:label": "Part of project",
      "rdfs:domain": [{"@id": "fairspace:Collection"}],
      "rdfs:range": [{"@id": "fairspace:ResearchProject"}],
      "fairspace:allowMultiple": false
    },
    {
      "@id": "fairspace:isProjectConsent",
      "@type": "rdf:Property",
      "rdfs:label": "Consent for specific project",
      "rdfs:domain": [{"@id": "fairspace:PersonConsent"}],
      "rdfs:range": [{"@id": "fairspace:ResearchProject"}],
      "fairspace:allowMultiple": true
    },
    {
      "@id": "fairspace:hasPersonConsent",
      "@type": "rdf:Property",
      "rdfs:label": "Person consent",
      "rdfs:domain": [{"@id": "fairspace:ResearchProject"}],
      "rdfs:range": [{"@id": "fairspace:PersonConsent"}],
      "fairspace:allowMultiple": true
    }
  ]
}
